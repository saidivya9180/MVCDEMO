                    <li class="nav-item">
                        <a href="#share-head-section" class="nav-link">Review&Analyze</a>
                    </li>

                </ul>
            </div>
        </div>
    </nav>
    <!-- HOME SECTION -->

    <header id="home-section">
        <div class="dark-overlay">

            <div class="home-inner">
                <div class="container">

                    <div class="row">
                        <div class="col-lg-8 d-none d-lg-block">

                            <div id="mySidenav" class="sidenav">
                                <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>
                                <ul class="list-unstyled components">
                                   
                                    <li>
                                        <a href="#" onclick="closeNavs()">Instructions</a>
                                    </li>
                                    

                                    <li>
                                        <a href="#pageSubmenu" data-toggle="collapse" aria-expanded="false" class="dropdown-toggle">My Profile</a>
